<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import java.net.URL?>
<VBox fx:id="root" alignment="CENTER" spacing="22"
      styleClass="alert-panel"
      xmlns:fx="http://javafx.com/fxml">
    <Label fx:id="headerLabel" styleClass="alert-header"/>
    <Label fx:id="contentLabel" styleClass="alert-content"/>
    <Button fx:id="okButton" text="OK" styleClass="button-main"/>
    <stylesheets>
        <URL value="@/css/auth_theme.css"/>
    </stylesheets>
    <stylesheets>
        <URL value="@/css/auth_theme.css"/>
    </stylesheets>
</VBox>
